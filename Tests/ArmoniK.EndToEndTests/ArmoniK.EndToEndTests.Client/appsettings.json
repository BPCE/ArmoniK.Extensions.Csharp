{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Grpc": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 1000 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "ArmoniK.EndToEndTests"
    }
  },
  "Components": {
  },

  "S3Storage": {
    "ServiceURL": "TODOEndPointS3",
    "AccessKeyId": "TODO AccessKeyId",
    "SecretAccessKey": "TheKey",
    "BucketName": "TheName"
  },
  "FileStorageType": "S3",

  "Grpc": {
    "EndPoint": "http://172.26.142.143:5001",
    "SSLValidation": "disable",
    "mTLS": "true"

  },
  "ApplicationConfig": {
    "DebugMode": false,
    "AppName": "ArmonikSamplesSymphony",
    "appVersion": "1.0.0"
  }
}
