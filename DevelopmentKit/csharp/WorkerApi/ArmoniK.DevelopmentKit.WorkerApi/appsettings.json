{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Volumes": {
    "target_app_path": "/app/data"
  },
  "ControlPlan": {
    "GrpcChannel": {
      "Address": "https://localhost:5001",
      "SocketType": "web"
    },
    "MessageBatchSize": 1
  }

}
