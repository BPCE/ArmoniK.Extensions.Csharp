{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Grpc": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 1000 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "ArmoniK.EndToEndTests"
    }
  },
  "Components": {
  },

  "S3Storage": {
    "ServiceURL": "TODOEndPointS3",
    "AccessKeyId": "TODO AccessKeyId",
    "SecretAccessKey": "TheKey",
    "BucketName": "TheName"
  },
  "FileStorageType": "S3",

  "Grpc": {
    "EndPoint": "https://ANEO-SB2-8454-wsl.local:5001",
    "SSLValidation": "disable",
    "CaCert": "../../../../infrastructure/quick-deploy/localhost/armonik/generated/ca.crt",
    "ClientCert": "../../../../infrastructure/quick-deploy/localhost/armonik/generated/client.crt",
    "ClientKey": "../../../../infrastructure/quick-deploy/localhost/armonik/generated/client.key"
  },
  "ApplicationConfig": {
    "DebugMode": false,
    "AppName": "ArmonikSamplesSymphony",
    "appVersion": "1.0.0"
  }
}